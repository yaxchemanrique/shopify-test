<footer>
  <div class="footer-container">
    <div class="footer-links">
      <nav>
        {% render 'main-nav' %}
      </nav>
      <div class="social-media-links">
        <ul>
          <li>
            <a href="#">
              <img width="31px" height="31px" src="{{ 'linkedin.svg' | asset_url }}">
            </a>
          </li>
          <li>
            <a href="#">
              <img width="31px" height="31px" src="{{ 'vimeo.svg' | asset_url }}">
            </a>
          </li>
          <li>
            <a href="#">
              <img width="31px" height="31px" src="{{ 'instagram.svg' | asset_url }}">
            </a>
          </li>
          <li>
            <a href="#">
              <img width="31px" height="31px" src="{{ 'twitter.svg' | asset_url }}">
            </a>
          </li>
          <li>
            <a href="#">
              <img width="31px" height="31px" src="{{ 'facebook.svg' | asset_url }}">
            </a>
          </li>
        </ul>
      </div>
    </div>
    <div class="logo">
      <a href="/">
        <img width="55px" height="55px" src="{{ 'logo-tennis-ball.svg' | asset_url }}">
      </a>
    </div>
  </div>
</footer>

